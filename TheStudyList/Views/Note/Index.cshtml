@using TheStudyList.Domain.Entities
@model IEnumerable<Note>

@{
    ViewBag.Title = "To Review";
}

<h2>@ViewBag.Title</h2>

<table class="table table-striped table-bordered">
    <tr>
        <th>Title</th>
        <th>T</th>
        <th>Due</th>
        <th>Ivl</th>
        <th>User</th>
    </tr>
    @foreach (var note in Model)
    {
        <tr>
            <td>@note.Title</td>
            <td>@note.TimeEstimate</td>
            <td>@note.DueDate.ToString("D")</td>
            <td>@note.LastInterval</td>
            <td>@note.User.UserName</td>
        </tr>
    }
</table>

