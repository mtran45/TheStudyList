
@{
    ViewBag.Title = "Import";
}

<h2>Import</h2>

@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.Label("Notebook")
        @Html.TextBox("notebook", null, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.Label("Insert TSV below (Format: Title, TimeEstimate, DueDate, Ivl)")
        @Html.TextArea("tsvString", new { @class = "form-control", id = "import-tsv"})
    </div>

    <div class="form-group">
        <input type="submit" value="Import!" class="btn btn-primary" />
    </div>

}

<div>
    @Html.ActionLink("Back to List", "StudyList")
</div>
